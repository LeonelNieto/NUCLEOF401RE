{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "make-clean",
        "type": "shell",
        "command": "make clean",
        "group": {
          "kind": "build",
          "isDefault": true
        }
      },
      {
        "label": "make-all",
        "dependsOrder": "sequence",
        "dependsOn": ["make-clean"],
        "type": "shell",
        "command": "make",
        "group": {
          "kind": "build",
          "isDefault": true
        }
      }
    ]
  }